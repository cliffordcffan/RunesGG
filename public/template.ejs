<body>
	<header>
		<div class = "wrap">
		<!--Dynamic Champion name taken from search from index-->
			<% if(championName !== null){ %>
					<a href='index.html'><img class='logo' src ='<%='../images/squares/'+championName+'Square.png'%>'/></a>
				<b class ="title" ><%= championName %></b>
			<% } %>

			<% if(error !== null){ %>
				<b><%= error %></b>
			<% } %>
		<!--end of dynamic name change-->
			<fieldset class="searchContainerSmall">
				<form action="/" method="post">
					<input name = "Champion" type="text" class="search-input-small" placeholder=" Enter champion name" required>
					<input type="submit" class="search-button-small" value="Search">
				</form>
			</fieldset>
		</div>
	</header>
	<div class = "RunesType" >
